{
  "name": "gekkard.front.v2",
  "private": true,
  "version": "2.0.84",
  "type": "module",
  "workspaces": [
    "./src/shared/(orval)api",
    "./src/shared/lib"
  ],
  "scripts": {
    "orval": "orval",
    "check": "tsc --noEmit && echo Done.",
    "local": "set APP_TYPE=GEKKARD && vite --mode dev.gekkard",
    "gko-local": "cross-env APP_TYPE=GEKKOIN vite --mode dev.gekkoin",
    "PRD-GEKKARD": "node increment-version && tsc && cross-env APP_TYPE=GEKKARD vite build --mode prd.gekkard",
    "PRD-GEKKOIN": "node increment-version && tsc && cross-env APP_TYPE=GEKKOIN vite build --mode prd.gekkoin",
    "STG-GEKKARD": "node increment-version && tsc && cross-env APP_TYPE=GEKKARD vite build --mode stg.gekkard",
    "STG-GEKKOIN": "node increment-version && tsc && cross-env APP_TYPE=GEKKOIN vite build --mode stg.gekkoin",
    "DEV-GEKKARD": "node increment-version && tsc && cross-env APP_TYPE=GEKKARD vite build --mode dev.gekkard",
    "DEV-GEKKOIN": "node increment-version && tsc && cross-env APP_TYPE=GEKKOIN vite build --mode dev.gekkoin",
    "DEV-GEKWALLET": "node increment-version && tsc && cross-env APP_TYPE=GEKWALLET vite build --mode dev.gekwallet"
  },
  "npm": " > 8",
  "node": " > 16",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/archivo": "^5.0.15",
    "@fontsource/azeret-mono": "^5.0.18",
    "@fontsource/inter": "^5.0.15",
    "@mui/material": "^5.14.20",
    "@stomp/stompjs": "^7.0.0",
    "antd": "5.2.3",
    "axios": "^1.6.7",
    "base64url": "^3.0.1",
    "crypto-js": "4.1.1",
    "date-fns": "2.29.3",
    "date-fns-tz": "2.0.0",
    "dayjs": "1.11.7",
    "decimal.js": "10.4.3",
    "elliptic": "^6.5.4",
    "firebase": "^10.3.0",
    "html5-qrcode": "^2.3.8",
    "i18next": "^23.6.0",
    "imask": "6.4.3",
    "js-sha256": "^0.11.0",
    "jsencrypt": "3.3.2",
    "md5": "^2.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^13.3.1",
    "react-international-phone": "^4.0.4",
    "react-phone-input-2": "2.15.1",
    "react-qr-code": "2.0.11",
    "react-router-dom": "6.8.2",
    "react-simple-pull-to-refresh": "^1.3.3",
    "sha256-uint8array": "^0.10.7",
    "usehooks-ts": "2.9.1",
    "uuid": "^9.0.1",
    "zustand": "4.3.6"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "@types/crypto-js": "^4.1.2",
    "@types/elliptic": "^6.4.18",
    "@types/node": "18.14.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-react": "3.1.0",
    "autoprefixer": "10.4.13",
    "orval": "^6.19.1",
    "path": "0.12.7",
    "postcss": "^8.4.35",
    "sass": "1.58.3",
    "tailwindcss": "3.2.7",
    "typescript": "4.9.5",
    "vite": "4.5.2",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-node-polyfills": "^0.21.0",
    "vite-plugin-pwa": "^0.17.4",
    "vite-plugin-svgr": "^4.1.0"
  }
}